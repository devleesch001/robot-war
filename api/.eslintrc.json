{
    "env": {
        "browser": false,
        "node": true,
        "es6": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "prettier/prettier": ["warn",  {"endOfLine": "lf"}],
        "max-len": [2, 120, 4, {"ignoreUrls": true}],
        "semi": ["error", "always"]
    }
}
